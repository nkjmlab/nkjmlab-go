      <exclusions>
        <exclusion>
          <groupId>*</groupId>
          <artifactId>*</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.webjars.npm</groupId>
      <artifactId>fortawesome__fontawesome-free</artifactId>
      <version>6.4.0</version>
      <exclusions>
        <exclusion>
