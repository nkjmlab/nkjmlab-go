  </licenses>
  <developers>
    <developer>
      <name>Yuu Nakajima</name>
      <email>yuu.nkjm.pub@gmail.com</email>
      <organization>nkjmlab.org</organization>
      <organizationUrl>https://github.com/nkjmlab</organizationUrl>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:git@github.com:nkjmlab/nkjmlab-go.git</connection>
    <url>https://github.com/nkjmlab/nkjmlab-go</url>
    <tag>HEAD</tag>
